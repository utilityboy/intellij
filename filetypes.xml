<application>
  <component name="CommunityFileTypes" version="11">
    <ignoreFiles list="CVS;SCCS;RCS;rcs;.DS_Store;.svn;.pyc;.pyo;*.pyc;*.pyo;.git;*.hprof;_svn;.hg;*.lib;*~;__pycache__;.bundle;*.rbc;" />
    <extensionMap>
      <mapping ext="env" type="PLAIN_TEXT" />
      <mapping ext="tagx" type="XML" />
      <mapping ext="jspx" type="XML" />
    </extensionMap>
  </component>
  <component name="FileTypeManager" version="17">
    <extensionMap>
      <mapping pattern="Gemfile" type="PLAIN_TEXT" />
      <mapping pattern=".env" type="PLAIN_TEXT" />
      <mapping pattern="Capfile" type="PLAIN_TEXT" />
      <mapping pattern="Procfile" type="PLAIN_TEXT" />
    </extensionMap>
  </component>
</application>